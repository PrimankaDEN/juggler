<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="main"/>

    <Button
        android:id="@+id/navigate_to_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="list"/>

    <Button
        android:id="@+id/navigate_to_about"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="about"/>

    <Button
        android:id="@+id/navigate_to_login"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="login"/>

    <Button
        android:id="@+id/navigate_to_wizzard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="wizzard"/>

    <Button
        android:id="@+id/navigate_to_toolbar_explain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="toolbar expalain"/>

    <Button
        android:id="@+id/navigate_to_infinity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="infinity"/>


    <Button
        android:id="@+id/navigate_to_preview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="preview"/>

</LinearLayout>